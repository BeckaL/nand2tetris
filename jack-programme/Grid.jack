class Grid {
    field array blocks; //blocks in grid
    field int size; //size of grid

    constructor Grid new() {
        var array arr;
        var Block block;
        let size = 4;
        let block = Block.new(2);
        let arr[0] = block;
        let blocks = arr;
        return this;
    }

    method void dispose() {
        do memory.deAlloc(this);
        return;
    }
}